{
  "name": "next-app-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css,less,sass,scss}'",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "dependencies": {
    "@apollo/client": "^3.4.9",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "graphql": "^15.5.1",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.3.0",
    "recharts": "^2.1.2",
    "sass": "^1.37.5",
    "swr": "^0.5.6",
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.9",
    "@types/react-grid-layout": "^1.1.2",
    "@types/recharts": "^2.0.0",
    "babel-jest": "^27.1.1",
    "eslint": "^7.30.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.1",
    "jest-watch-typeahead": "^0.6.4",
    "prettier": "^2.4.0",
    "typescript": "^4.4.2"
  }
}
